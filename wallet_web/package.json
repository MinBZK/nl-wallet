    "dist"
  ],
  "main": "./dist/nl-wallet-web.umd.cjs",
  "module": "./dist/nl-wallet-web.js",
  "exports": {
    ".": {
      "import": "./dist/nl-wallet-web.js",
      "require": "./dist/nl-wallet-web.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
