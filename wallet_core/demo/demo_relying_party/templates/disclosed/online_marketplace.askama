{% let username = Some(attributes | attribute("given_name")) %}
{% call header::header(base.trans.marketplace_title, username) %}

<main>
    {% let welcome = base.trans.welcome %}
    {% call notification::notification(format!("{welcome} {}", username.unwrap()), "", "") %}
    {% call search::search(base.trans.search_by_topic) %}
    {% call products::products(6) %}
</main>
