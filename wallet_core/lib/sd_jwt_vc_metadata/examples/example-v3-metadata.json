{
  "vct": "https://sd_jwt_vc_metadata.example.com/example_credential_v3",
  "name": "NL Wallet PID credential v3",
  "description": "Working version of the NL Wallet PID credential v3",
  "extends": "https://sd_jwt_vc_metadata.example.com/example_credential_v2",
  "extends#integrity": "sha256-wrBJMDL2BmMi/s/siFTekDcIfGxF8OJEyX0ldjTQbow=",
  "claims": [
    {
      "path": ["birth_date"],
      "sd": "always"
    },
    {
      "path": ["place_of_birth", "locality"],
      "sd": "always"
    },
    {
      "path": ["place_of_birth", "country", "name"],
      "sd": "always"
    },
    {
      "path": ["place_of_birth", "country", "area_code"],
      "sd": "never"
    },
    {
      "path": ["place_of_birth", "country", "is_eu_member"],
      "sd": "never"
    },
    {
      "path": ["financial", "has_debt"],
      "sd": "always"
    },
    {
      "path": ["financial", "debt_amount"],
      "sd": "always"
    },
    {
      "path": ["financial", "has_job"],
      "sd": "always"
    },
    {
      "path": ["nickname"],
      "sd": "always",
      "svg_id": "nickname"
    }
  ],
  "schema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "NL Wallet extended PID VCT Schema",
    "description": "The JSON schema that defines the NL Wallet extended PID VCT",
    "type": "object",
    "properties": {
      "birth_date": {
        "type": "string",
        "format": "date"
      },
      "place_of_birth": {
        "type": "object",
        "properties": {
          "locality": {
            "type": "string"
          },
          "country": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "area_code": {
                "type": "integer"
              },
              "is_eu_member": {
                "type": "boolean"
              }
            }
          }
        }
      },
      "financial": {
        "type": "object",
        "properties": {
          "has_debt": {
            "type": "boolean"
          },
          "has_job": {
            "type": "boolean"
          },
          "debt_amount": {
            "type": "integer"
          }
        }
      },
      "nickname": {
        "type": "string"
      },
      "vct": {
        "type": "string"
      },
      "vct#integrity": {
        "type": "string"
      },
      "iss": {
        "type": "string"
      },
      "iat": {
        "type": "number"
      },
      "exp": {
        "type": "number"
      },
      "nbf": {
        "type": "number"
      }
    },
    "required": ["vct", "iss", "iat", "attestation_qualification"]
  }
}
